count_dead  <- round(df_KM_3wk[i,]$Daily_Count_Dead)  # raw counts
loopDF           <- data.frame(matrix(0,ncol = 4, nrow = (count_alive + count_dead)))
colnames(loopDF) <- (c('Date','Treatment','Tank_number','Count_dead'))
